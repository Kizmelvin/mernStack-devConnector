n\n// Add Likes\nexport const addLike = (id) => async (dispatch) => {\n  const res = await axios.put(`api/posts/like/${id}`);\n  try {\n    dispatch({\n      type: UPDATE_LIKES,\n      payload: { id, likes: res.data },\n    });\n  } catch (err) {\n    dispatch({\n      type: POST_ERROR,\n      payload: {\n        msg: err.response.statusText,\n        status: err.response.status,\n      },\n    });\n  }\n};\n\n// Remove Likes\nexport const removeLike = (id) => async (dispatch) => {\n  const res = await axios.put(`api/posts/unlike/${id}`);\n  try {\n    dispatch({\n      type: UPDATE_LIKES,\n      payload: { id, likes: res.data },\n    });\n  } catch (err) {\n    dispatch({\n      type: POST_ERROR,\n      payload: { msg: err.response.statusText, status: err.response.status },\n    });\n  }\n};\n\n// Delete Post\nexport const deletePost = (id) => async (dispatch) => {\n  await axios.delete(`api/posts/${id}`);\n  try {\n    dispatch({\n      type: DELETE_POST,\n      payload: id,\n    });\n    dispatch(setAlert(\"Post Deleted\", \"success\"));\n  } catch (err) {\n    dispatch({\n      type: POST_ERROR,\n      payload: { msg: err.response.statusText, status: err.response.status },\n    });\n  }\n};\n\n// Add Post\nexport const addPost = (postText) => async (dispatch) => {\n  const config = {\n    headers: {\n      \"Content-Type\": \"application/json\",\n    },\n  };\n  const res = await axios.post(\"api/posts\", postText, config);\n  try {\n    dispatch({\n      type: ADD_POST,\n      payload: res.data,\n    });\n    dispatch(setAlert(\"Post Created\", \"success\"));\n  } catch (err) {\n    dispatch({\n      type: POST_ERROR,\n      payload: { msg: err.response.statusText, status: err.response.status },\n    });\n  }\n};\n\n// Add Comment\nexport const addComment = (commentText, postId) => async (dispatch) => {\n  const config = {\n    headers: {\n      \"Content-Type\": \"application/json\",\n    },\n  };\n  const res = await axios.post(\n    `api/posts/comments/${postId}`,\n    commentText,\n    config\n  );\n  try {\n    dispatch({\n      type: ADD_COMMENT,\n      payload: res.data,\n    });\n    dispatch(setAlert(\"Comment Added\", \"success\"));\n  } catch (err) {\n    dispatch({\n      type: POST_ERROR,\n      payload: { msg: err.response.statusText, status: err.response.status },\n    });\n  }\n};\n\n// Remove Comment\nexport const removeComment = (postId, commentId) => async (dispatch) => {\n  const res = await axios.delete(`api/posts/comments/${postId}/${commentId}`);\n  try {\n    dispatch({\n      type: REMOVE_COMMENT,\n      payload: commentId,\n    });\n    dispatch(setAlert(\"Comment Deleted\", \"success\"));\n  } catch (err) {\n    dispatch({\n      type: POST_ERROR,\n      payload: { msg: err.response.statusText, status: err.response.status },\n    });\n  }\n};\n","/Users/kenny/Desktop/Kizmelvin.dev/react-projects/mern-modern/client/src/components/posts/Post.js",[],"/Users/kenny/Desktop/Kizmelvin.dev/react-projects/mern-modern/client/src/components/posts/PostItem.js",[],"/Users/kenny/Desktop/Kizmelvin.dev/react-projects/mern-modern/client/src/components/posts/PostForm.js",[],"/Users/kenny/Desktop/Kizmelvin.dev/react-projects/mern-modern/client/src/components/post/CommentForm.js",[],"/Users/kenny/Desktop/Kizmelvin.dev/react-projects/mern-modern/client/src/components/post/CommentItem.js",["198"],{"ruleId":"199","replacedBy":"200"},{"ruleId":"201","replacedBy":"202"},{"ruleId":"203","severity":1,"message":"204","line":5,"column":1,"nodeType":"205","endLine":16,"endColumn":2},{"ruleId":"203","severity":1,"message":"204","line":18,"column":1,"nodeType":"205","endLine":54,"endColumn":2},{"ruleId":"203","severity":1,"message":"204","line":18,"column":1,"nodeType":"205","endLine":57,"endColumn":2},{"ruleId":"206","severity":1,"message":"207","line":46,"column":6,"nodeType":"208","endLine":46,"endColumn":34,"suggestions":"209"},{"ruleId":"206","severity":1,"message":"210","line":10,"column":6,"nodeType":"208","endLine":10,